{
    "name": "datalogix/laravel-hash-fields",
    "description": "Laravel Hash Fields automatically hash â€‹model fields.",
    "keywords": [
        "laravel",
        "hash",
        "fields"
    ],
    "homepage": "https://github.com/datalogix/laravel-hash-fields",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ricardo Gobbo de Souza",
            "email": "ricardogobbosouza@yahoo.com.br"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.7",
        "illuminate/database": "^7.0|^8.0|^9.0",
        "mockery/mockery": "^1.4",
        "phpunit/phpunit": "^8.5|^9.5",
        "spatie/phpunit-watcher": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Datalogix\\HashFields\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Datalogix\\HashFields\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "watch": "vendor/bin/phpunit-watcher watch"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
